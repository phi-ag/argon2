{
  "type": "module",
  "name": "@phi-ag/argon2",
  "version": "0.5.6",
  "license": "MIT",
  "description": "Minimal Argon2 WebAssembly SIMD build",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/argon2.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "argon2",
    "crypto",
    "hashing",
    "password",
    "wasm",
    "webassembly",
    "simd",
    "emscripten"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./fetch": "./dist/fetch.js",
    "./argon2.wasm": "./dist/argon2.wasm"
  },
  "scripts": {
    "build": "tsc && cp -f src/argon2.wasm ./dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "test:e2e": "playwright test",
    "test:e2e:dev": "PWTEST_WATCH=1 playwright test",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,css,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,css,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test",
    "sbom": "node utils/sbom.ts"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-library": "8.6.0",
    "@eslint/js": "9.35.0",
    "@playwright/test": "1.55.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/libsodium-wrappers-sumo": "0.7.8",
    "@types/node": "24.5.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.35.0",
    "globals": "16.4.0",
    "hash-wasm": "4.12.0",
    "libsodium-wrappers-sumo": "0.7.15",
    "packageurl-js": "2.0.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "uuid": "13.0.0",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
}
